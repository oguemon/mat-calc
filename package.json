{
  "name": "calc-tmp",
  "version": "1.0.0",
  "description": "SASSとjsのコンパイル。",
  "private": true,
  "scripts": {
    "dev:server": "vite",
    "dev:sass": "sass --no-source-map -w sass/style.scss out/style.css",
    "build": "run-s build:ts build:sass",
    "build:ts": "vite build",
    "build:sass": "sass --no-source-map --style=compressed sass/style.scss out/style.css",
    "lint:type": "tsc --noEmit"
  },
  "dependencies": {
    "@types/jquery": "3.5.18",
    "@types/mathjax": "0.0.37",
    "jquery": "3.7.1",
    "mathjax": "3.2.2",
    "sass": "1.66.1",
    "typescript": "5.2.2",
    "vite": "4.4.9"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}
